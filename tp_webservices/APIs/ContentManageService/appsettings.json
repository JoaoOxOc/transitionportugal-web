{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnectionString": "Host=host.docker.internal:5433;Database=cmsservicedb;Username=postgres;Password=postgres"
  },
  "DatabaseSettings": {
    "DbHost": "host.docker.internal",
    "DbPort": "5433",
    "Database": "cmsservicedb",
    "DbUser": "postgres",
    "DbPassword": "postgres",
    "DbPooling": "false",
    "DbServerCompatibilityMode": "Redshift",
    "DbTrustCertificate": "true",
    "DbSslMode": "None"
  },
  "ApplicationSettings": {
    "RabbitMQHost": "tprabbitmq",
    "RabbitMQUser": "guest",
    "RabbitMQPassword": "guest",
    "RabbitMQExchanges": "push-email-exchange,push-exception-exchange",
    "RabbitMQEmailQueue": "emails-on-queue",
    "RabbitMQExceptionQueue": "exceptions-on-queue"
  },
  "AllowedHosts": "*"
}
